#include <stdio.h>
#include <conio.h>

void hoanvi(int &x, int &y);
void hoanvi(int &x, int &y){
	int temp = x;
	x = y;
	y = temp;
}

int main(){
	int x,y;
	printf("Nhap x: ");
	scanf("%d",&x);
	printf("Nhap y: ");
	scanf("%d",&y);
	hoanvi(x,y);
	printf("Gia tri cua x hien tai la: %d\n", x);
	printf("Gia tri cua y hien tai la: %d\n", y);
	getch();
	return 0;
}
